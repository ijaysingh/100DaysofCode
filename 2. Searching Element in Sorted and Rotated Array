n = int(input("Enter no. of Elements "))
print("Enter Numbers ")
arr = list(map(int,input().split()))
target = int(input("Enter Target "))
l = 0
r = n-1
while(l <= r):
    mid = (l + r) // 2
    if arr[mid] == target:
        print("The Index is", mid)
        break
    elif arr[mid] >= arr[l]:
        if target >= arr[l] and target <= arr[mid]:
            r = mid - 1
        else:
            l = mid + 1
    elif arr[mid] <= arr[r]:
        if target >= arr[mid] and target <= arr[r]:
            l = mid + 1
        else:
            r = mid - 1 
else:
    print("Not Found")
